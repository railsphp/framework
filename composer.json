{
    "name": "railsphp/framework",
    "description": "Yet another Rails-like framework for PHP",
    "keywords": [
        "rails",
        "php",
        "railsphp",
        "framework"
    ],
    "license": "MIT",
    
    "require": {
        "php": ">=5.4.0",
        "zendframework/zend-validator": "2.3.*",
        "zendframework/zend-mail": "2.3.*",
        "zendframework/zend-console": "2.3.*",
        "zendframework/zend-db": "2.3.*",
        "zendframework/zend-paginator": "2.3.*",
        "symfony/yaml": "2.3.*",
        "symfony/console": "2.3.*",
        "pagon/jade": "1.0.*",
        "nesbot/carbon": "1.8.*",
        "d11wtq/boris": "1.0.*",
        "nikic/php-parser": "0.9.4"
    },
    
    "require-dev": {
        "toopay/assetic-minifier": "1.3.*",
        "leafo/scssphp": "0.0.8",
        "coffeescript/coffeescript": "1.3.1"
    },
    
    "autoload": {
        "psr-0": {
            "Rails\\": "src/"
        }
    }
}
